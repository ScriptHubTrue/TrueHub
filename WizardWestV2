do local Rayfield=loadstring(game:HttpGet("https://sirius.menu/rayfield"))();local UserInputService=game:GetService("UserInputService");local RunService=game:GetService("RunService");local LocalPlayer=game.Players.LocalPlayer;local Camera=workspace.CurrentCamera;_G.aimActive=false;_G.lockedTarget=nil;_G.rightMouseDown=false;local Window=Rayfield:CreateWindow({Name="TrueHub",Icon=0,LoadingTitle="TrueHub Loading",LoadingSubtitle="by Secret",Theme="Amethyst",DisableRayfieldPrompts=false,DisableBuildWarnings=false,ConfigurationSaving={Enabled=true,FolderName=nil,FileName="TrueHub"},Discord={Enabled=false,Invite="noinvitelink",RememberJoins=true},KeySystem=false,KeySettings={Title="Untitled",Subtitle="Key System",Note="No method of obtaining the key is provided",FileName="Key",SaveKey=true,GrabKeyFromSite=false,Key={"Hello"}}});local FarmTab=Window:CreateTab("Farm",4483362458);local AimTab=Window:CreateTab("Aim",1234567890);local SellTab=Window:CreateTab("Sell",9876543210);local StaySafeToggle=FarmTab:CreateToggle({Name="StaySafe",CurrentValue=false,Flag="Toggle3",Callback=function(Value) _G.staySafe=Value;end});local AutoFarmToggle=FarmTab:CreateToggle({Name="Autofarm",CurrentValue=false,Flag="Toggle1",Callback=function(Value) _G.toggleActive=Value;if Value then local player=game.Players.LocalPlayer;local character=player.Character or player.CharacterAdded:Wait() ;local hrp=character:WaitForChild("HumanoidRootPart");local originalCFrame=hrp.CFrame;local visitedWagon={};local entities=game.Workspace:FindFirstChild("Entities");if  not entities then warn("Entities not found in Workspace!");return;end local function getUnvisited(objectName,visitedTable) local list={};for _,item in ipairs(entities:GetChildren()) do if ((item.Name==objectName) and  not visitedTable[item]) then table.insert(list,item);end end return list;end while _G.toggleActive do local wagonItems=getUnvisited("WagonLoot",visitedWagon);if ( #wagonItems>0) then for _,wagon in ipairs(wagonItems) do if  not _G.toggleActive then break;end visitedWagon[wagon]=true;local targetCFrame;if wagon:IsA("BasePart") then targetCFrame=wagon.CFrame;elseif wagon:IsA("Model") then targetCFrame=(wagon.PrimaryPart and wagon.PrimaryPart.CFrame) or wagon:GetModelCFrame() ;end if targetCFrame then hrp.CFrame=targetCFrame;wait(2);local prompt=wagon:FindFirstChildWhichIsA("ProximityPrompt");if ( not prompt and wagon:IsA("Model") and wagon.PrimaryPart) then prompt=wagon.PrimaryPart:FindFirstChildWhichIsA("ProximityPrompt");end if prompt then if (typeof(fireproximityprompt)=="function") then fireproximityprompt(prompt);else wait(prompt.HoldDuration + 0.1 );end end if _G.staySafe then hrp.CFrame=CFrame.new(978.31,15.61, -1240.45);else hrp.CFrame=originalCFrame;end wait(2);end end end wait(1);end if _G.staySafe then hrp.CFrame=CFrame.new(978.31,15.61, -1240.45);else hrp.CFrame=originalCFrame;end end end});local MissionsToggle=FarmTab:CreateToggle({Name="Missions(Wanted)",CurrentValue=false,Flag="Toggle4",Callback=function(Value) _G.MissionsActive=Value;if Value then spawn(function() local player=game.Players.LocalPlayer;local character=player.Character or player.CharacterAdded:Wait() ;local hrp=character:WaitForChild("HumanoidRootPart");local originalCFrame=hrp.CFrame;local visitedWagonMissions={};local visitedArtifactMissions={};while _G.MissionsActive do local missionsFolder=game.Workspace:FindFirstChild("Missions");if missionsFolder then local artifactMission=missionsFolder:FindFirstChild("ArtifactMission");if artifactMission then local lootFolder=artifactMission:FindFirstChild("Loot");if lootFolder then local wagonLoots={};local artifactMissions={};for _,item in ipairs(lootFolder:GetChildren()) do if (item.Name=="WagonLoot") then if  not visitedWagonMissions[item] then table.insert(wagonLoots,item);end elseif (item.Name=="Pillar") then local artifact=item:FindFirstChild("Artifact");if (artifact and  not visitedArtifactMissions[artifact]) then table.insert(artifactMissions,artifact);end end end